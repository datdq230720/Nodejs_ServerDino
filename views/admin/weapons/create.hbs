<script>

    function validateForm() {
        var name = document.forms["contactForm"]["name"].value;
        var image = document.forms["contactForm"]["image"].value;
        var point = document.forms["contactForm"]["point"].value;
        var dame = document.forms["contactForm"]["dame"].value;
        var code = document.forms["contactForm"]["code"].value;
        var describes = document.forms["contactForm"]["describes"].value;

        if (name == null || name == "") {
            alert("Chưa nhập tên vũ khí");
            return false;
        } else if (image == null || image == "") {
            alert("Chưa có hình ảnh vũ khí");
            return false;
        }
        else if (point == null || point == "") {
            alert("Chưa nhập số điểm để mở khóa");
            return false;
        }
        else if (dame == null || dame == "") {
            alert("Chưa nhập chỉ số sát thương của vũ khí");
            return false;
        }
        else if (code == null || code == "") {
            alert("Chưa nhập số thứ tự của vũ khí");
            return false;
        }
        else if (describes == null || describes == "") {
            alert("Chưa nhập mô tả vũ khí");
            return false;
        }
    }
</script>