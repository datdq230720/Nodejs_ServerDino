<script>

    function validateForm() {
        var name = document.forms["contactForm"]["name"].value;
        var image = document.forms["contactForm"]["image"].value;
        var category = document.forms["contactForm"]["category"].value;
        var hp = document.forms["contactForm"]["hp"].value;
        var describes = document.forms["contactForm"]["describes"].value;

        if (name == null || name == "") {
            alert("Chưa nhập tên quái vật");
            return false;
        } else if (image == null || image == "") {
            alert("Chưa có hình ảnh quái vật");
            return false;
        }
        else if (category == null || category == "") {
            alert("Chưa nhập loại quái vật");
            return false;
        }
        else if (hp == null || hp == "") {
            alert("Chưa nhập số máu của quái vật");
            return false;
        }
        else if (describes == null || describes == "") {
            alert("Chưa nhập mô tả quái vật");
            return false;
        }
    }
</script>